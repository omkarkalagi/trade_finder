    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.5",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^3.4.1",
